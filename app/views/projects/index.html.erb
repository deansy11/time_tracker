<h1>Projects Index</h1>
<h2>These are your projects</h2>
<td class="button"><%= link_to 'Add a Project', new_project_path %></td>
<%= link_to "Log Out", session_path, method: :delete %>

<table>
  <thead>
    <tr>
      <th class="headers">Project Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
    <tbody>
        <% @projects.each do |project| %>
          <tr class="body">
            <td><%= project.name %></td>
            <td class="button"><%= link_to 'Show', project %></td>
            <td class="button"><%= link_to 'Edit', edit_project_path(project) %></td>
            <td class="button"><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
    </tbody>
</table>
